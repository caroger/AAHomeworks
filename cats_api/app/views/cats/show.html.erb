<h1>Cat Name: <%= @cat.name %></h1>
<h1>Skillz : <%= @cat.skill %></h1>
<h1>Toys Owned: </h1>
<ul>
  <% @cat.toys.each do |toy| %>
    <li><%= toy.name %></li>
  <% end %>
</ul>
<li>
  <a href="<%= new_cat_toy_url(@cat) %>">Create New Toy</a>
</li>
<li>
  <a href="<%= edit_cat_url(@cat) %>">Update Cat</a>
</li>
<li>
  <a href=<%= cats_url %>>Index</a>
</li>
